{
	"name": "tag-breakdown-generator",
	"version": "1.0.3",
	"description": "Breakdown nested tags into multiple parent tags",
	"main": "main.js",
	"scripts": {
		"dev": "bun esbuild.config.mjs",
		"build": "tsc -noEmit && bun esbuild.config.mjs production",
		"version": "bun version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"tag",
		"generator",
		"plugin",
		"markdown",
		"yaml",
		"frontmatter",
		"typescript",
		"bun"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/diff-match-patch": "^1.0.34",
		"@types/js-yaml": "^4.0.6",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"ts-unused-exports": "^10.0.1",
		"tslib": "2.4.0",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"diff-match-patch": "^1.0.5",
		"js-yaml": "^4.1.0",
		"mdast-util-from-markdown": "^1.2.0",
